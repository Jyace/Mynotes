<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jyace.mynotes">
<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_photo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.jyace.mynotes.com.tops.notes.Login"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".com.tops.notes.AtyEditNote"></activity>
<activity android:name=".com.tops.notes.MainActivity"></activity>
<activity android:name=".com.tops.notes.AtyVideoViewer"></activity>
        <activity android:name=".com.tops.notes.AtyPhotoViewer"></activity>

    </application>

</manifest>
